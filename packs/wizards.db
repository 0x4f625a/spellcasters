creature"},"range":{"value":"touch"},"area":null,"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"applyMod":true,"type":{"categories":[],"value":"negative"},"value":"1d4"}}},"save":{"value":"fortitude","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"location":{"value":"CwpACdxUuSriusIU"},"hasCounteractCheck":{"value":false},"ability":{"value":""},"heightening":{"damage":{"0":"1d4"},"interval":1,"type":"interval"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"ownership":{"default":0,"aTWxsSrAlxp06IfB":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"VAtRGRxXZP2b4ig6","name":"Gale Blast","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/gale-blast.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.dDiOnjcsBFbAvP6t"}},"system":{"description":{"gm":"","value":"<p>Wind flows from your outstretched hands and whirls around you in a 5-foot emanation. Each creature in the area takes bludgeoning damage equal to your spellcasting ability modifier, with a Fortitude save.</p>\n<hr />\n<p><strong>Critical Success</strong> The creature is unaffected.</p>\n<p><strong>Success</strong> The creature takes half damage.</p>\n<p><strong>Failure</strong> The creature takes full damage and is pushed 5 feet away from you.</p>\n<p><strong>Critical Failure</strong> The creature takes double damage and is pushed 10 feet away from you.</p>\n<hr />\n<p><strong>Heightened (+2)</strong> The damage increases by 1d6.</p>"},"source":{"value":"Pathfinder Secrets of Magic"},"rules":[],"slug":"gale-blast","schema":{"version":0.835,"lastMigration":{"datetime":"2023-03-31T20:40:07.240-04:00","version":{"schema":0.769,"foundry":"10.291","system":"4.10.1"}}},"traits":{"value":["air","cantrip"],"rarity":"common"},"level":{"va