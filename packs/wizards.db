ocation"},"components":{"focus":false,"material":false,"somatic":false,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":10,"type":"emanation"},"time":{"value":"1"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"untyped","subtype":""},"value":"1d6","applyMod":false}}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"location":{"value":"kJS7AYyX00HEKfJ6"},"hasCounteractCheck":{"value":false},"ability":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"ownership":{"default":0,"aTWxsSrAlxp06IfB":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"FIp3gQIXVElX8srr","name":"Electric Arc","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/electric-arc.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.kBhaPuzLUSwS6vVf"}},"system":{"description":{"gm":"","value":"<p>An arc of lightning leaps from one target to another. You deal electricity damage equal to 1d4 plus your spellcasting ability modifier.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 1d4.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"rules":[],"slug":"electric-arc","schema":{"version":0.835,"lastMigration":{"datetime":"2023-03-31T20:40:07.046-04:00","version":{"schema":0.769,"foundry":"10.291","system":"4.10.1"}}},"traits":{"value":["electricity","cantrip"],"rarity":"common"},"level":{"value":1},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","primal"],"custom":""},"school":{"value":"evocation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 or 2 creatures"},"range":{"value":"30 feet"},"area":null,"time":