","fire"],"rarity":"common","custom":""},"rules":[],"slug":"blazing-dive","schema":{"version":0.769,"lastMigration":null},"level":{"value":3},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","primal"],"custom":""},"school":{"value":"evocation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"10","areaType":"emanation"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"applyMod":false,"type":{"subtype":"","value":"bludgeoning"},"value":"3d4"},"1":{"applyMod":false,"type":{"categories":[],"subtype":"","value":"fire"},"value":"3d6"}},"applyMod":false},"save":{"value":"reflex","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K4cNToNFh8WRRTsJ"},"hasCounteractCheck":{"value":false},"heightening":{"damage":{"0":"1d4","1":"1d6"},"interval":1,"type":"interval"},"prepared":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.KEkPxx7Sm6Xf4W3s"}}},{"_id":"Z9YjfssWy0t7nFkb","name":"Lightning Bolt","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/lightning-bolt.webp","data":{"description":{"value":"<p>A bolt of lightning strikes outward from your hand, dealing 4d12 electricity damage.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 1d12.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["electricity"],"rarity":"common","custom":""},"rules":[],"slug":"lightning-bolt","schema":{"version":0.769,"lastMigration":null},"level":{"value":3},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","primal"]},"school":{"value":"evocation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"targ