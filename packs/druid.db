omatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"10","areaType":"emanation"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"applyMod":false,"type":{"subtype":"","value":"bludgeoning"},"value":"3d4"},"1":{"applyMod":false,"type":{"categories":[],"subtype":"","value":"fire"},"value":"3d6"}},"applyMod":false},"save":{"value":"reflex","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K4cNToNFh8WRRTsJ"},"hasCounteractCheck":{"value":false},"heightening":{"damage":{"0":"1d4","1":"1d6"},"interval":1,"type":"interval"},"prepared":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.KEkPxx7Sm6Xf4W3s"}}},{"_id":"Z9YjfssWy0t7nFkb","name":"Lightning Bolt","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/lightning-bolt.webp","data":{"description":{"value":"<p>A bolt of lightning strikes outward from your hand, dealing 4d12 electricity damage.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 1d12.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["electricity"],"rarity":"common","custom":""},"rules":[],"slug":"lightning-bolt","schema":{"version":0.769,"lastMigration":null},"level":{"value":3},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","primal"]},"school":{"value":"evocation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"120","areaType":"line"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"applyMod":false,"type":{"categories":[],"subtype":"","value":"electricity"},"value":"4d12"}}},"save":{"value":"reflex","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K4cNToNFh8WRRTsJ"},"hasCounteractCheck":{"value":false},"areasize":{"value":"120-foot line"},"heightening":{"damage":{"0":"1d12"},"interval":1,"type":"interval"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.9AAkVUCwF6WVNNY2"}}},{"_id":"Hoq1halzuSMhHXal","name":"Slow","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/slow.webp","data":{"description":{"value":"<p>You dilate the flow of time around the target, slowing its actions.</p>\n<hr />\n<p><strong>Critical Success</strong> The target is unaffected.</p>\n<p><strong>Success</strong> The target is @Compendium[pf2e.conditionitems.xYTAsEpcJE1Ccni3]{Slowed 1} for 1 round.</p>\n<p><strong>Failure</strong> The target is @Compendium[pf2e.conditionitems.xYTAsEpcJE1Ccni3]{Slowed 1} for 1 minute.</p>\n<p><strong>Critical Failure</strong> The target is @Compendium[pf2e.conditionitems.xYTAsEpcJE1Ccni3]{Slowed 2} for 1 minute.</p>\n<hr />\n<p><strong>Heightened (6th)</strong> You can target up to 10 creatures.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"slow","schema":{"version":0.769,"lastMigration":null},"level":{"value":3},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","occult","primal"]},"school":{"value":"transmutation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"30 feet"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":"1 minute"},"damage":{"value":{}},"save":{"value":"fortitude","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"K4cNToNFh8WRRTsJ"},"hasCounteractCheck":{"value":false},"areasize":{"value":""},"heightening":{"levels":{"6":{"target":{"value":"10 creatures"}}},"type":"fixed"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.WsUwpfmhKrKwoIe3"}}},{"_id":"mMW3w3rGSyw9SUwx","name":"Ice Storm","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/ice-storm.webp","data":{"description":{"value":"<p>You create a gray storm cloud that pelts creatures with an icy deluge. When you @Compendium[pf2e.actionspf2e.aBQ8ajvEBByv45yz]{Cast the Spell}, a burst of magical hail deals [[/r {2d8}[bludgeoning]]]{2d8 bludgeoning damage} and [[/r {2d8}[cold]]]{2d8 cold damage} to each creature in the area below the cloud (basic Reflex save). Snow and sleet continue to rain down in the area for the remainder of the spell's duration, making the area difficult terrain, causing creatures in the storm to be @Compendium[pf2e.conditionitems.DmAIPqOBomZ7H95W]{Concealed}, and making creatures outside the storm Concealed from those inside the storm. Any creature that ends its turn in the storm takes [[/r {2}[cold]]]{2 cold damage}. If you Cast this Spell outdoors, you can create two non-overlapping clouds instead of one. As normal, if a Large or larger creature is in both clouds, it still only takes the initial damage once and the continuing damage once per turn.</p>\n<hr />\n<p><strong>Heightened (+2)</strong> The initial bludgeoning damage and cold damage increase by 1d8 each, and the cold damage creatures take at the end of their turns increases by 1.</p>"},"source":{"value":"Pathfinder Advanced Player's Guide"},"traits":{"value":["cold"],"rarity":"common","custom":""},"rules":[],"slug":"ice-storm","schema":{"version":0.769,"lastMigration":null},"level":{"value":4},"spell