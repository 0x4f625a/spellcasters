:""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"negative"},"value":"6d6"}}},"save":{"value":"fortitude","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"r0Bgn7C6blCbNnTz"},"hasCounteractCheck":{"value":false},"areasize":{"value":""},"heightening":{"damage":{"0":"2d6"},"interval":1,"type":"interval"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.N1Z1oLPdBxaSgrEE"},"scene-packer":{"hash":"76a738c9609ad02f3433dc0279b654c006e741da"}}},{"_id":"gIhWXciuxkPP4HRi","name":"Enervation","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/enervation.webp","data":{"description":{"value":"<p>Extending a finger, you fire a beam of negative energy that weakens the life force of any creature it touches. Each living creature in the line must attempt a Fortitude save.</p>\n<hr />\n<p><strong>Critical Success</strong> The creature is unaffected.</p>\n<p><strong>Success</strong> The creature takes [[/r {2d8}[persistent,negative]]] @Compendium[pf2e.conditionitems.lDVqvLKA6eF3Df60]{Persistent Negative Damage}.</p>\n<p><strong>Failure</strong> The creature takes [[/r {4d8}[persistent,negative]]] @Compendium[pf2e.conditionitems.lDVqvLKA6eF3Df60]{Persistent Negative Damage} and becomes @Compendium[pf2e.conditionitems.4D2KBtexWXa6oUMR]{Drained 1}.</p>\n<p><strong>Critical Failure</strong> The creature takes [[/r {4d8}[persistent,negative]]] @Compendium[pf2e.conditionitems.lDVqvLKA6eF3Df60]{Persistent Negative Damage} and becomes @Compendium[pf2e.conditionitems.4D2KBtexWXa6oUMR]{Drained 2} and @Compendium[pf2e.conditionitems.3uh1r86TzbQvosxv]{Doomed 1}.</p>\n<hr />\n<p><strong>Heightened (+2)</strong> The persistent negative damage increases by [[/r {1d8}[persistent,negative]]] @Compendium[pf2e.conditionitems.lDVqvLKA6eF3Df60]{Persistent Negative Damage} on a success, or by [[/r {2d8}[persistent,negative]]] @Compendium[pf2e.conditionitems.lDVqvLKA6eF3Df60]{Persistent Negative Damage} on a failure or critical failure.</p>"},"source":{"value":"Pathfinder Advanced Player's Guide"},"traits":{"value":["negative"],"rarity":"common","custom":""},"rules":[],"slug":"enervation","schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-05T10:18:05.527-04:00","version":{"schema":0.768,"foundry":"9.269","system":"3.13.5.16"}}},"level":{"value":4},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"]},"school":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":30,"areaType":"line"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"negative"},"value":"4d8 persistent"}}},"save":{"value":"fortitude","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"r0Bgn7C6blCbNnTz"},"hasCounteractCheck":{"value":false},"areasize":{"value":"30-foot line"},"heightening":{"damage":{"0":"2d8"},"interval":2,"type":"interval"},"prepared":{"value":false},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.eexkxcqnkXazsGfK"},"scene-packer":{"hash":"80b2304e7dd6121b964c9f88f05f4ef186bd9b89"}}},{"_id":"eSEG3JpidraAvy5g","name":"Painful Vibrations","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/painful-vibrations.webp","data":{"description":{"value":"<p>You send powerful sound waves through an opponent's body, vibrating its bones and internal organs painfully. The creature takes 8d6 sonic damage and must attempt a Fortitude save.</p>\n<hr />\n<p><strong>Critical Success</strong> The target is unaffected.</p>\n<p><strong>Success</strong> The target takes half damage.</p>\n<p><strong>Failure</strong> The target takes full damage, is @Compendium[pf2e.conditionitems.fesd1n5eVhpCSS18]{Sickened 1}, and is @Compendium[pf2e.conditionitems.9PR9y0bi4JPKnHPR]{Deafened} for 1 round.</p>\n<p><strong>Critical Failure</strong> The target takes double damage, is @Compendium[pf2e.conditionitems.fesd1n5eVhpCSS18]{Sickened 2}, and is deafened for 1 minute.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 2d6.</p>"},"source":{"value":"Pathfinder Secrets of Magic"},"traits":{"value":["sonic"],"rarity":"common","custom":""},"rules":[],"slug":"painful-vibrations","schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-05T10:18:05.527-04:00","version":{"schema":0.768,"foundry":"9.269","system":"3.13.5.16"}}},"level":{"value":4},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["divine","occult"],"custom":""},"school":{"value":"evocation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 living creature"},"range":{"value":"100 feet"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"applyMod":false,"type":{"subtype":"","value":"sonic"},"value":"8d6"}},"applyMod":false},"save":{"value":"fortitude","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"r0Bgn7C6blCbNnTz"},"hasCounteractCheck":{"value":false},"heightening":{"damage":{"0":"2d6"},"interval":1,"type":"interval"}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.qTzvVnAMWL05VitC"},"scene-packer":{"hash":"8f783e8aedbdf3cb91721443f40387becba1797e"}}},{"_id":"I9nNcPXDCA9mkP9T","name":"Vampiric Maiden","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/vampiric-maiden.webp","data":{"description":{"value":"<p>A ghostly iron maiden snaps shut on the target and drains its vitality for your gain. This deals [[/r {4d4}[piercing]]]{4d4 piercing damage} and [[/r {4d4}[negative]]]{4d4 negative damage}, and the target must attempt a Fortitude save. You gain temporary HP equal to the negative damage the target takes (after applying resistances, weaknesses, and the like). You lose any remaining temporary HP after 1 minute.</p>\n<hr />\n<p><strong>Critical Success</strong> The target is unaffected.</p>\n<p><strong>Success</strong> The target takes half damage.</p>\n<p><strong>Failure</strong> The target is briefly trapped within the vampiric maiden. The target takes full damage and is @Compendium[pf2e.conditionitems.eIcWbB5o3pP6OIMe]{Immobilized} by the iron maiden for 1 round or until it uses an @Compendium[pf2e.actionspf2e.pvQ5rY2zrtPI614F]{Interact} action to extricate itself, whichever comes first.</p>\n<p><strong>Critical Failure</strong> The target takes double damage and is Immobilized by the vampiric maiden for 1 round or until it @Compendium[pf2e.actionspf2e.SkZAQRkLLkmBQNB9]{Escapes}(the DC is your spell DC), whichever comes first.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The piercing and negative damage increase by [[/r 1d4]] each.</p>"},"source":{"value":"Pathfinder Advanced Player's Guide"},"traits":{"value":["negative"],"rarity":"common","custom":""},"rules":[],"slug":"vampiric-maiden","schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-05T10:18:05.527-04:00","version":{"schema":0.768,"foundry":"9.269","system":"3.13.5.16"}}},"level":{"value":4},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"]},"school":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"30 feet"},"area":{"value":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"piercing"},"value":"4d4 + 4d4"}}},"save":{"value":"fortitude","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"r0Bgn7C6blCbNnTz"},"hasCounteractCheck":{"value":false},"areasize":{"value":""},"heightening":{"damage":{"0":"1d4+1d4"},"interval":1,"type":"interval"},"prepared":{"value":false},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.07xYlmGX32XtHGEt"},"scene-packer":{"hash":"ddb91699d52c551743939c731a44f9385724e691"}}},{"_id":"aAPj13sN8Bia1C4I","name":"Abyssal Plague","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/abyssal-plague.webp","data":{"description":{"value":"<p>Your touch afflicts the target with Abyssal plague, which siphons fragments of their soul away to empower the Abyss. The effect is based on the target's Fortitude save.</p>\n<hr />\n<p><strong>Critical Success</strong> The target is unaffected.</p>\n<p><strong>Success</strong> The target takes 2 evil damage per spell level, and takes a -2 status penalty to saves against Abyssal plague for 1 day or until the target contracts it, whichever comes first.</p>\n<p><strong>Failure</strong> The target is afflicted with Abyssal plague at stage 1.</p>\n<p><strong>Critical Failure</strong> The target is afflicted with Abyssal plague at stage 2.</p>\n<hr />\n<p><strong>Abyssal Plague</strong> (disease) <strong>Level 9</strong></p>\n<p>The target can't recover from the drained condition from Abyssal plague until the disease is cured</p>\n<p><strong>Stage 1</strong> @Compendium[pf2e.conditionitems.4D2KBtexWXa6oUMR]{Drained 1} (1 day)</p>\n<p><strong>Stage 2</strong> Drained increases by 2 (1 day).</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["chaotic","disease","evil"],"rarity":"common","custom":""},"rules":[],"slug":"abyssal-plague","schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-05T10:18:05.527-04:00","version":{"schema":0.768,"foundry":"9.269","system":"3.13.5.16"}}},"level":{"value":5},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["divine","occult"]},"school":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"touch"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{}},"save":{"value":"fortitude","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"r0Bgn7C6blCbNnTz"},"hasCounteractCheck":{"value":false},"areasize":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.vJuaxTd6q11OjGqA"},"scene-packer":{"hash":"14a9d762d0830004a78f12b2f61497ad3ff9e95a"}}},{"_id":"SuAVJ1ta9Zr0lyJS","name":"Mirror Malefactors","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/mirror-malefactors.webp","data":{"description":{"value":"<p>You completely surround a Large or smaller creature in a shimmering ring of illusory mirrors that follows it as it moves, even when the creature moves outside the range of the spell. These mirrors reflect the target, though if the target has no reflection, like a vampire, it's unaffected by <em>mirror malefactors</em>. As long as the target is surrounded by mirrors, it's @Compendium[pf2e.conditionitems.TBSHQspnbcqxsmjL]{Frightened 1}, and it can't reduce its frightened value.</p>\n<p>When you Cast the Spell, the reflections leap from the mirrors and attack the target, dealing 7d8 mental damage with a basic Will save. On subsequent turns, this effect repeats the first time you Sustain the spell that turn. If the target succeeds at any Will save it attempts against <em>mirror malefactors</em>, the illusory mirrors shatter and the spell ends.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage for both the initial effect and on subsequent turns increases by 1d8.</p>"},"source":{"value":"Pathfinder Secrets of Magic"},"traits":{"value":["mental","visual"],"rarity":"common","custom":""},"rules":[],"slug":"mirror-malefactors","schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-05T10:18:05.527-04:00","version":{"schema":0.768,"foundry":"9.269","system":"3.13.5.16"}}},"level":{"value":5},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"],"custom":""},"school":{"value":"illusion"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"1 creature"},"range":{"value":"30 feet"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":"sustained up to 1 minute"},"damage":{"value":{"0":{"applyMod":false,"type":{"subtype":"","value":"mental"},"value":"7d8"}},"applyMod":false},"save":{"value":"will","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"r0Bgn7C6blCbNnTz"},"hasCounteractCheck":{"value":false},"heightening":{"damage":{"0":"1d8"},"interval":1,"type":"interval"}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.CmZCq4htcZ6W0TKk"},"scene-packer":{"hash":"012b31332b08788695bd4c29d54401182a341bdc"}}},{"_id":"p8wL6AC01SqwHHRf","name":"Rip the Spirit","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/rip-the-spirit.webp","data":{"description":{"value":"<p>You supernaturally rip the spirit from a living creature's body, dooming the target to pain and death. The target takes 5d6 negative damage, depending on its basic Fortitude save, and is @Compendium[pf2e.conditionitems.4D2KBtexWXa6oUMR]{Drained 1} if it fails its save. The spell's effect is based on how many actions you spend when Casting the Spell.</p>\n<p><span class=\"pf2-icon\">1</span> <strong>(somatic)</strong> The spell targets one living creature in range.</p>\n<p><span class=\"pf2-icon\">2</span> <strong>(somatic, verbal)</strong> The spell targets one living creature in range and the damage is [[/r {10d6}[negative]]]{10d6 negative damage} instead of 5d6.</p>\n<p><span class=\"pf2-icon\">3</span> <strong>(material, somatic, verbal)</strong> The spell targets all living creatures in a 30-foot emanation.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The amount of damage increases by 1d6, or by [[/r 2d6]] for the 2-action version.</p>"},"source":{"value":"Pathfinder Secrets of Magic"},"traits":{"value":["death","negative"],"rarity":"common","custom":""},"rules":[],"slug":"rip-the-spirit","schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-05T10:18:05.527-04:00","version":{"schema":0.768,"foundry":"9.269","system":"3.13.5.16"}}},"level":{"value":5},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["divine","occult"],"custom":""},"school":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":false,"verbal":false},"materials":{"value":""},"target":{"value":"1 living creature"},"range":{"value":"30 feet"},"area":{"value":"","areaType":""},"time":{"value":"1 to 3"},"duration":{"value":""},"damage":{"value":{"0":{"applyMod":false,"type":{"subtype":"","value":"negative"},"value":"5d6"}},"applyMod":false},"save":{"value":"fortitude","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"r0Bgn7C6blCbNnTz"},"hasCounteractCheck":{"value":false},"heightening":{"damage":{"0":"1d6"},"interval":1,"type":"interval"},"overlays":{"7dx7zf6stp6umc4r":{"_id":"7dx7zf6stp6umc4r","data":{"area":{"areaType":"emanation","value":"30"},"components":{"material":true,"somatic":true,"verbal":true},"target":{"value":"All living creatures"},"time":{"value":"3"}},"overlayType":"override","sort":3},"jma2elwwr2saf9s6":{"_id":"jma2elwwr2saf9s6","data":{"components":{"somatic":true},"time":{"value":"1"}},"overlayType":"override","sort":1},"jq9loelhf38up0e1":{"_id":"jq9loelhf38up0e1","data":{"components":{"somatic":true,"verbal":true},"damage":{"value":{"0":{"value":"10d6"}}},"heightening":{"damage":{"0":"2d6"}},"time":{"value":"2"}},"overlayType":"override","sort":2}}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.1bw6XJMOERcbC5Iq"},"scene-packer":{"hash":"61df2e0658ff57151e90a640a7fb8b05c7d11bde"}}},{"_id":"5KxuOX5ja55WtKFN","name":"Blade Barrier","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/blade-barrier.webp","data":{"description":{"value":"<p>Blades of force form a churning wall. The wall is a straight line 20 feet high, 120 feet long, and 2 inches thick, and it provides cover. The wall deals 7d8 force damage to each creature that's in the wall's space when it is created, that attempts to pass through the wall, or that ends its turn inside the wall. A basic Reflex save reduces the damage. A creature that succeeds at this save when the wall is created is pushed to the nearest space on the side of its choice. Creatures trying to move through the wall fail to do so if they critically fail the save, ending their movement adjacent to the wall.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 1d8.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["force"],"rarity":"common","custom":""},"rules":[],"slug":"blade-barrier","schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-05T10:18:05.527-04:00","version":{"schema":0.768,"foundry":"9.269","system":"3.13.5.16"}}},"level":{"value":6},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["divine"]},"school":{"value":"evocation"},"components":{"focus":false,"material":true,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"120 feet"},"area":{"value":null,"areaType":""},"time":{"value":"3"},"duration":{"value":"1 minute"},"damage":{"value":{"0":{"type":{"categories":[],"value":"force"},"value":"7d8"}}},"save":{"value":"reflex","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"r0Bgn7C6blCbNnTz"},"hasCounteractCheck":{"value":false},"areasize":{"value":""},"heightening":{"damage":{"0":"1d8"},"interval":1,"type":"interval"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.peCF6VArm8urfwxZ"},"scene-packer":{"hash":"2631a9f0f6994878db1de6f7afd633d13862d372"}}},{"_id":"BMA11pTW71T2UgDJ","name":"Devil Form","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/devil-form.webp","data":{"description":{"value":"<p>You bind yourself to the power of Hell, transforming into a Medium devil battle form. When you cast this spell, choose barbazu, erinys, osyluth, or sarglagon. If you choose osyluth or sarglagon, the battle form is Large and you must have enough space to expand into or the spell is lost. While in this form you gain the devil and fiend traits. You have hands in this battle form and can use manipulate actions. You can Dismiss the spell.</p>\n<p>You gain the following statistics and abilities regardless of the form that you choose:</p>\n<ul>\n<li>AC = 22 + your level. Ignore your armor check's penalty and Speed reduction.</li>\n<li>5 temporary Hit Points, resistance 5 to physical damage (except silver), and weakness 5 to good.</li>\n<li>Resistance 10 to fire.</li>\n<li>Darkvision.</li>\n<li>One or more attacks specific to the battle form you use. You're trained with them. Your attack modifier is +20, and you use the listed damage. These attacks are Strength based (for the purpose of the enfeebled condition, for example). If your attack modifier is higher for the given unarmed attack or weapon, you can use it instead.</li>\n<li>Athletics modifier of +23, unless your own modifier is higher.</li>\n</ul>\n<p>You also gain specific abilities based on the type of devil you choose:</p>\n<ul>\n<li><strong>Barbazu</strong> Speed 35 feet;\n<ul>\n<li><strong>Melee</strong> <span class=\"pf2-icon\">1</span> glaive (deadly d8, forceful, reach 10 feet), <strong>Damage</strong> 2d8+10 slashing plus 1d6 evil and 1d6 persistent bleed;</li>\n<li><strong>Melee</strong> <span class=\"pf2-icon\">1</span> beard, <strong>Damage</strong> 3d8 piercing plus 1d6 evil;</li>\n<li><strong>Melee</strong> <span class=\"pf2-icon\">1</span> claw (agile), Damage 3d6 slashing plus 1d6 evil.</li>\n</ul>\n</li>\n</ul>\n<p>@Compendium[pf2e.spell-effects.0yy4t4UY1HqrEo70]{Spell Effect: Devil Form (Barbazu)}</p>\n<ul>\n<li><strong>Erinys</strong> Speed 25 feet, fly 40 feet;\n<ul>\n<li><strong>Melee</strong> <span class=\"pf2-icon\">1</span> longsword (versatile P), <strong>Damage</strong> 1d8+10 slashing plus 1d6 evil and 1d6 fire;</li>\n<li><strong>Ranged</strong> <span class=\"pf2-icon\">1</span> composite longbow (deadly d10, range increment 100 feet, volley), <strong>Damage</strong> 1d8 piercing plus 1d6 evil and 1d6 fire.</li>\n</ul>\n</li>\n</ul>\n<p>@Compendium[pf2e.spell-effects.BDMEqBsumguTrMXa]{Spell Effect: Devil Form (Erinys)}</p>\n<ul>\n<li><strong>Osyluth</strong> Speed 35 feet, fly 30 feet;\n<ul>\n<li><strong>Melee</strong> <span class=\"pf2-icon\">1</span> jaws, <strong>Damage</strong> 2d10+10 piercing plus 1d6 evil;</li>\n<li><strong>Melee</strong> <span class=\"pf2-icon\">1 </span>claw (agile, reach 10 feet), <strong>Damage</strong> 2d6 slashing plus 1d6 evil;</li>\n<li><strong>Melee</strong> <span class=\"pf2-icon\">1</span> stinger (reach 15 feet), <strong>Damage</strong> 1d10 piercing plus 1d6 evil and 1d6 poison;</li>\n<li><strong>Ranged</strong> <span class=\"pf2-icon\">1</span> bone shard (range increment 30 feet), <strong>Damage</strong> 2d6 piercing plus 1d6 evil.</li>\n</ul>\n</li>\n</ul>\n<p>@Compendium[pf2e.spell-effects.HEbbxKtBzsLhFead]{Spell Effect: Devil Form (Osyluth)}</p>\n<ul>\n<li><strong>Sarglagon</strong> Speed 25 feet, fly 25 feet, swim 30 feet;\n<ul>\n<li><strong>Melee</strong> <span class=\"pf2-icon\">1</span> fangs, <strong>Damage</strong> 2d10+10 piercing plus 1d6 evil;</li>\n<li><strong>Melee</strong> <span class=\"pf2-icon\">1</span> tentacle arm (agile), <strong>Damage</strong> 1d8 bludgeoning plus 1d6 evil and 1d6 poison.</li>\n</ul>\n</li>\n</ul>\n<p>@Compendium[pf2e.spell-effects.fKeZDm8kpDFK5HWp]{Spell Effect: Devil Form (Sarglagon)}</p>"},"source":{"value":"Pathfinder Secrets of Magic"},"traits":{"value":["evil","lawful","polymorph"],"rarity":"common","custom":""},"rules":[],"slug":"devil-form","schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-05T10:18:05.527-04:00","version":{"schema":0.768,"foundry":"9.269","system":"3.13.5.16"}}},"level":{"value":6},"spellType":{"value":"utility"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine"],"custom":""},"school":{"value":"transmutation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":"1 minute"},"damage":{"value":{}},"save":{"value":"","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"r0Bgn7C6blCbNnTz"},"hasCounteractCheck":{"value":false},"prepared":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.dR53f0p15EW7D0xC"},"scene-packer":{"hash":"7cbf4f07621dd0fcb50dd4c8bba21d1ba44ae23f"}}},{"_id":"gP1NMwIxBCwbuNKW","name":"Vampiric Exsanguination","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/vampiric-exsanguination.webp","data":{"description":{"value":"<p>You draw in the blood and life force of other creatures through your outstretched arms. You deal 12d6 negative damage to living creatures in the area. You gain temporary Hit Points equal to half the damage a single creature takes from this spell; calculate these temporary Hit Points using the creature that took the most damage. You lose any remaining temporary Hit Points after 1 minute.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 2d6.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["death","negative"],"rarity":"common","custom":""},"rules":[],"slug":"vampiric-exsanguination","schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-05T10:18:05.527-04:00","version":{"schema":0.768,"foundry":"9.269","system":"3.13.5.16"}}},"level":{"value":6},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"]},"school":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":""},"area":{"value":"30","areaType":"cone"},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"negative"},"value":"12d6"}}},"save":{"value":"fortitude","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"r0Bgn7C6blCbNnTz"},"hasCounteractCheck":{"value":false},"areasize":{"value":"30-foot cone"},"heightening":{"damage":{"0":"2d6"},"interval":1,"type":"interval"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.fd31tAHSSGXyOxW6"},"scene-packer":{"hash":"8286718fe085701dc880019efd19349b036bc8b6"}}},{"_id":"LcTcJXg0RJDvxhtW","name":"Divine Decree","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/divine-decree.webp","data":{"description":{"value":"<p>You utter a potent litany from your faith, a mandate that harms those who oppose your ideals. Choose an alignment your deity has (chaotic, evil, good, or lawful). You can't cast this spell if you don't have a deity or your deity is true neutral. This spell gains the trait of the alignment you chose. You deal 7d10 damage to creatures in the area; each creature must attempt a Fortitude save. Creatures with an alignment that matches the one you chose are unaffected by the spell. Those that neither match nor oppose it treat the result of their saving throw as one degree better and don't suffer effects other than damage.</p>\n<hr />\n<p><strong>Critical Success</strong> The creature is unaffected.</p>\n<p><strong>Success</strong> The creature takes half damage.</p>\n<p><strong>Failure</strong> The creature takes full damage and is @Compendium[pf2e.conditionitems.MIRkyAjyBeXivMa7]{Enfeebled 2} for 1 minute.</p>\n<p><strong>Critical Failure</strong> The creature takes double damage and is @Compendium[pf2e.conditionitems.MIRkyAjyBeXivMa7]{Enfeebled 2} for 1 minute. On your home plane, a creature that critically fails is banished with the effect of a failed <em>@Compendium[pf2e.spells-srd.bay4AfSu2iIozNNW]{Banishment}</em> save. A 10th-level creature or lower must attempt a Will save. On a failure, it's @Compendium[pf2e.conditionitems.6uEgoh53GbXuHpTF]{Paralyzed} for 1 minute; on a critical failure, it dies.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 1d10, and the level of creatures that must attempt a second save on a critical failure increases by 2.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"divine-decree","schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-05T10:18:05.527-04:00","version":{"schema":0.768,"foundry":"9.269","system":"3.13.5.16"}}},"level":{"value":7},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["divine"]},"school":{"value":"evocation"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"40 feet"},"area":{"value":"40","areaType":"emanation"},"time":{"value":"2"},"duration":{"value":"varies"},"damage":{"value":{"0":{"applyMod":false,"type":{"categories":[],"subtype":"","value":"untyped"},"value":"7d10"}}},"save":{"value":"fortitude","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"r0Bgn7C6blCbNnTz"},"hasCounteractCheck":{"value":false},"areasize":{"value":"40-foot emanation"},"prepared":{"value":""},"primarycheck":{"value":""},"secondarycasters":{"value":""},"secondarycheck":{"value":""}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.sX2o0HH4RjJDAZ8C"},"scene-packer":{"hash":"f73e05df86c0d855f24825e8bacfc5aadc509992"}}},{"_id":"Fb9OkODzqUfMbeJU","name":"Inexhaustible Cynicism","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/inexhaustible-cynicism.webp","data":{"description":{"value":"<p>You inflict your targets with a crushing doubt about everyone and everything around them. For the duration, each target is unwilling to receive any effects that require a willing target (in case the effect was secretly an attack), refuse to ingest anything (in case the substances have been replaced with poisons and cursed duplicates), doesn't treat anyone as an ally (in case its friends were replaced by an impostor), and doesn't believe anything it's told (in case of lies, though this doesn't mean the creature automatically believes the opposite either). It doesn't even fully trust its own ability to Recall Knowledge, as it becomes convinced it most likely remembered false information, as from a critical failure on a check to Recall Knowledge.</p>\n<p>Finally, the creature takes 6d6 mental damage at the end of each of its turns (with no save) unless it spent at least 1 action that turn Seeking or otherwise interacting with its surroundings in an attempt to disbelieve the illusions that it feels are obviously surrounding it.</p>\n<hr />\n<p><strong>Critical Success</strong> The creature is unaffected.</p>\n<p><strong>Success</strong> The creature is affected for 1 round.</p>\n<p><strong>Failure</strong> The creature is affected for 1 minute.</p>\n<p><strong>Critical Failure</strong> The creature is affected for 1 hour.</p>"},"source":{"value":"Pathfinder Secrets of Magic"},"traits":{"value":["emotion","mental"],"rarity":"common","custom":""},"rules":[],"slug":"inexhaustible-cynicism","schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-05T10:18:05.527-04:00","version":{"schema":0.768,"foundry":"9.269","system":"3.13.5.16"}}},"level":{"value":7},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"],"custom":""},"school":{"value":"enchantment"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":"up to 5 creatures"},"range":{"value":"30 feet"},"area":{"value":"","areaType":""},"time":{"value":"2"},"duration":{"value":""},"damage":{"value":{"0":{"applyMod":false,"type":{"subtype":"","value":"mental"},"value":"6d6"}},"applyMod":false},"save":{"value":"will","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"r0Bgn7C6blCbNnTz"},"hasCounteractCheck":{"value":false}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.gMbrNFKz9NP3TR4s"},"scene-packer":{"hash":"4918637e8b4df546a9f0d7897943ba0cebd51f67"}}},{"_id":"rblPorHIKBHHxhOI","name":"Tempest of Shades","type":"spell","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/spells/tempest-of-shades.webp","data":{"description":{"value":"<p>You channel the forces of undeath to briefly call forth an amalgam of the vengeful dead slain by your enemies and allies alike. This amalgam manifests as a large tornado of insubstantial, howling faces. It occupies the space of a Huge creature and has a Speed of 60 feet.</p>\n<p><strong>Arrive</strong> (negative) <em>Vortex of Death</em> Each enemy creature within a 60-foot emanation must attempt a Fortitude save with the following effects.</p>\n<hr />\n<p><strong>Critical Success</strong> The creature is unaffected</p>\n<p><strong>Success</strong> The creature is @Compendium[pf2e.conditionitems.4D2KBtexWXa6oUMR]{Drained 1}.</p>\n<p><strong>Failure</strong> The creature is @Compendium[pf2e.conditionitems.4D2KBtexWXa6oUMR]{Drained 2}.</p>\n<p><strong>Critical Failure</strong> The creature is @Compendium[pf2e.conditionitems.4D2KBtexWXa6oUMR]{Drained 3}.</p>\n<hr />\n<p><strong>Depart</strong> (emotion, fear, mental) <em>Howl of Eternity</em> The vengeful dead lets out an anguished scream. All your enemies within a 100-foot emanation must attempt @Check[type:will|name:Tempest of Shades Depart|traits:necromancy,emotion,fear,mental] saves with the following effects.</p>\n<hr />\n<p><strong>Critical Success</strong> The creature is unaffected.</p>\n<p><strong>Success</strong> The creature is @Compendium[pf2e.conditionitems.TBSHQspnbcqxsmjL]{Frightened 1}.</p>\n<p><strong>Failure</strong> The creature is @Compendium[pf2e.conditionitems.TBSHQspnbcqxsmjL]{Frightened 3}.</p>\n<p><strong>Critical Failure</strong> The creature is frightened 3. It's also @Compendium[pf2e.conditionitems.sDPxOjQ9kx2RZE8D]{Fleeing} for 1 round or until it's no longer frightened, whichever comes first.</p>"},"source":{"value":"Pathfinder Secrets of Magic"},"traits":{"value":["incarnate"],"rarity":"common","custom":""},"rules":[],"slug":"tempest-of-shades","schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-05T10:18:05.527-04:00","version":{"schema":0.768,"foundry":"9.269","system":"3.13.5.16"}}},"level":{"value":7},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"],"custom":""},"school":{"value":"necromancy"},"components":{"focus":false,"material":true,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"100 feet"},"area":{"value":"","areaType":""},"time":{"value":"3"},"duration":{"value":"until the end of your next turn"},"damage":{"value":{}},"save":{"value":"fortitude","basic":""},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"r0Bgn7C6blCbNnTz"},"hasCounteractCheck":{"value":false}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"aTWxsSrAlxp06IfB":3},"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.JLdbyGKhjwAAoRLs"},"scene-packer":{"hash":"9a08955d885c5d44dd784f6252e015c8ad9e3efc"}}},{"_id":"y1sAvTzV0g6UHspg","name":"Occult Innate Spells","type":"spellcastingEntry","img":"https://assets.forge-vtt.com/bazaar/systems/pf2e/assets/icons/default-icons/spellcastingEntry.svg","data":{"description":{"value":""},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.769,"lastMigration":{"datetime":"2022-08-05T22:15:25.276-04:00","version":{"schem